{
  "author": "focus@kleegroup.com",
  "name": "my-awesome-project-to-rename-app",
  "description": "Focus demonstration application",
  "version": "0.0.0",
  "homepage": "https://github.com/KleeGroup/focus-demo-app",
  "repository": {
    "type": "git",
    "url": "https://github.com/KleeGroup/focus-demo-app.git"
  },
  "engines": {
    "node": "6.9.x",
    "npm": "3.10.x"
  },
  "scripts": {
    "production": "better-npm-run production",
    "bundle": "better-npm-run bundle",
    "build": "npm run bundle",
    "start": "npm run start:npm",
    "start:local": "better-npm-run webpack-dev-server-local-focus",
    "start:npm": "better-npm-run webpack-dev-server-npm-focus",
    "watch": "better-npm-run watch",
    "profile": "better-npm-run profile"
  },
  "main": "app/index.js",
  "betterScripts": {
    "bundle": {
      "command": "builder run focus-webpack-bundle",
      "env": {
        "ANCHOR_CLASS": "demo-app",
        "BABELIFIED_PATH": "./app",
        "ENTRY_FILE_PATH": "./app/index",
        "PAGE_TITLE": "Your project application",
        "API_HOST": "localhost",
        "API_PORT": 8080,
        "OUTPUT_DIR": "./",
        "OUTPUT_PUBLIC_PATH": "./",
        "GENERATE_HTML": false,
        "LOCAL_FOCUS": false,
        "LEGACY_SEARCH_API": false,
        "DEV": false,
        "NODE_ENV": "production",
        "BABEL_ENV": "production",
        "BASE_URL": "/",
        "MINIMIFY": false
      }
    },
    "profile": {
      "command": "builder run focus-webpack-profile",
      "env": {
        "ANCHOR_CLASS": "demo-app",
        "BABELIFIED_PATH": "./app",
        "ENTRY_FILE_PATH": "./app/index",
        "PAGE_TITLE": "Project application application",
        "API_HOST": "localhost",
        "API_PORT": 8080,
        "OUTPUT_DIR": "./public",
        "LOCAL_FOCUS": true,
        "LEGACY_SEARCH_API": false,
        "BASE_URL": "/"
      }
    },
    "watch": {
      "command": "builder run focus-webpack-watch",
      "env": {
        "ANCHOR_CLASS": "demo-app",
        "BABELIFIED_PATH": "./app",
        "ENTRY_FILE_PATH": "./app/index",
        "PAGE_TITLE": "Focus demonstration application",
        "API_HOST": "localhost",
        "API_PORT": 8080,
        "OUTPUT_DIR": "./public",
        "LOCAL_FOCUS": false,
        "LEGACY_SEARCH_API": false,
        "BASE_URL": "/"
      }
    },
    "production": {
      "command": "builder run focus-webpack-bundle",
      "env": {
        "ANCHOR_CLASS": "demo-app",
        "BABELIFIED_PATH": "./app",
        "ENTRY_FILE_PATH": "./app/index",
        "PAGE_TITLE": "Focus demonstration application",
        "OUTPUT_DIR": "./production",
        "LOCAL_FOCUS": false,
        "LEGACY_SEARCH_API": false,
        "BASE_URL": "/",
        "API_ROOT": "/",
        "DEV": false,
        "SOURCE_MAPS": false,
        "MINIMIFY": true,
        "NODE_ENV": "production",
        "BABEL_ENV": "production",
        "GENERATE_HTML": false
      }
    },
    "webpack-dev-server-local-focus": {
      "command": "builder run focus-dev-server",
      "env": {
        "ANCHOR_CLASS": "demo-app",
        "BABELIFIED_PATH": "./app",
        "ENTRY_FILE_PATH": "./app/index",
        "PAGE_TITLE": "Focus demonstration application",
        "API_HOST": "localhost",
        "API_PORT": 8080,
        "OUTPUT_DIR": "./public",
        "LOCAL_FOCUS": true,
        "LEGACY_SEARCH_API": false,
        "BASE_URL": "/"
      }
    },
    "webpack-dev-server-npm-focus": {
      "command": "builder run focus-dev-server",
      "env": {
        "ANCHOR_CLASS": "demo-app",
        "BABELIFIED_PATH": "./app",
        "ENTRY_FILE_PATH": "./app/index",
        "PAGE_TITLE": "Focus demonstration application",
        "API_HOST": "localhost",
        "API_PORT": 8080,
        "OUTPUT_DIR": "./public",
        "LOCAL_FOCUS": false,
        "LEGACY_SEARCH_API": false,
        "BASE_URL": "/"
      }
    }
  },
  "license": "MIT",
  "devDependencies": {
    "animate.css": "^3.4.0",
    "better-npm-run": "0.0.13",
    "builder": "^3.2.1",
    "eslint-config-focus": "git://github.com/KleeGroup/eslint-config-focus.git#updating-refreshing",
    "focus-components": "^2.1.1",
    "focus-core": "^2.1.1",
    "focus-devtools": "0.4.0",
    "lodash": "^4.17.3",
    "material-design-icons-iconfont": "^3.0.3",
    "moment": "^2.16.0",
    "native-promise-only": "^0.8.0-a",
    "numeral": "^1.5.6",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^3.0.0",
    "webpack-focus": "git://github.com/KleeGroup/webpack-focus.git#builder-archetype"
  }
}
